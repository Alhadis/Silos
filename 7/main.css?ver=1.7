/*
 * Basic stylinh
 */

.lsf,
.lsf-icon:before {
  font-family: 'LigatureSymbols';
  -webkit-text-rendering: optimizeLegibility;
  -moz-text-rendering: optimizeLegibility;
  -ms-text-rendering: optimizeLegibility;
  -o-text-rendering: optimizeLegibility;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-font-smoothing: antialiased;
  -ms-font-smoothing: antialiased;
  -o-font-smoothing: antialiased;
  font-smoothing: antialiased;
  font-weight: normal !important;
  font-style: normal !important;
  text-transform: none !important;
  font-size: 150%;
}
.lsf-icon:before {
  content: attr(title);
  margin-right: 0.3em;
  font-size: 130%;
}
.lsf-icon.amazon:before {
  content: '\E007';
}
input,
input[type=text],
textarea {
  -webkit-border-radius: 0 0 0 0;
  -khtml-border-radius: 0 0 0 0;
  -moz-border-radius: 0 0 0 0;
  -o-border-radius: 0 0 0 0;
  border-radius: 0 0 0 0;
}
b {
  font-weight: bold;
}
i {
  font-style: italic;
}
html,
body {
  width: 100%;
  height: 100%;
  background-color: #f0f0f0;
  font-size: 12px;
  line-height: 18px;
}
html::-webkit-scrollbar,
body::-webkit-scrollbar {
  width: 10px;
  background: #444;
}
html::-webkit-scrollbar-thumb,
body::-webkit-scrollbar-thumb {
  background: #111;
}
html::-webkit-scrollbar-thumb:hover,
body::-webkit-scrollbar-thumb:hover {
  background: #222;
}
.button,
a.button,
button,
input[type=submit] {
  font-family: "Open Sans", sans-serif;
  padding: 10px;
  font-size: 10px;
  line-height: 10px;
  background: #333;
  background: #333333;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #333333), color-stop(1, #444444));
  background: -ms-linear-gradient(bottom, #333333, #444444);
  background: -moz-linear-gradient(center bottom, #333333 0%, #444444 100%);
  background: -o-linear-gradient(#444444, #333333);
  color: #f0f0f0;
  text-decoration: none;
  text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.7);
  border: 1px solid #333;
  text-transform: uppercase;
  font-weight: 800;
  text-align: center;
  display: block;
  float: left;
  cursor: pointer;
}
.button:hover,
a.button:hover,
button:hover,
input[type=submit]:hover {
  background: #3f3f3f;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #3f3f3f), color-stop(1, #4f4f4f));
  background: -ms-linear-gradient(bottom, #3f3f3f, #4f4f4f);
  background: -moz-linear-gradient(center bottom, #3f3f3f 0%, #4f4f4f 100%);
  background: -o-linear-gradient(#4f4f4f, #3f3f3f);
}
.button:active,
a.button:active,
button:active,
input[type=submit]:active {
  background: #333333;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #333333), color-stop(1, #222222));
  background: -ms-linear-gradient(bottom, #333333, #222222);
  background: -moz-linear-gradient(center bottom, #333333 0%, #222222 100%);
  background: -o-linear-gradient(#222222, #333333);
}
.button.secondary,
a.button.secondary,
button.secondary,
input[type=submit].secondary {
  color: #333;
  text-shadow: 0 0 0;
  background: #bbbbbb;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #bbbbbb), color-stop(1, #cccccc));
  background: -ms-linear-gradient(bottom, #bbbbbb, #cccccc);
  background: -moz-linear-gradient(center bottom, #bbbbbb 0%, #cccccc 100%);
  background: -o-linear-gradient(#cccccc, #bbbbbb);
  border-color: #bbb;
  font-weight: 600;
}
.button.secondary:hover,
a.button.secondary:hover,
button.secondary:hover,
input[type=submit].secondary:hover {
  background: #bfbfbf;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #bfbfbf), color-stop(1, #cfcfcf));
  background: -ms-linear-gradient(bottom, #bfbfbf, #cfcfcf);
  background: -moz-linear-gradient(center bottom, #bfbfbf 0%, #cfcfcf 100%);
  background: -o-linear-gradient(#cfcfcf, #bfbfbf);
}
.button.secondary:active,
a.button.secondary:active,
button.secondary:active,
input[type=submit].secondary:active {
  background: #bbbbbb;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #bbbbbb), color-stop(1, #aaaaaa));
  background: -ms-linear-gradient(bottom, #bbbbbb, #aaaaaa);
  background: -moz-linear-gradient(center bottom, #bbbbbb 0%, #aaaaaa 100%);
  background: -o-linear-gradient(#aaaaaa, #bbbbbb);
}
.button.remove,
a.button.remove,
button.remove,
input[type=submit].remove {
  background: #860d07;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #860d07), color-stop(1, #b61209));
  background: -ms-linear-gradient(bottom, #860d07, #b61209);
  background: -moz-linear-gradient(center bottom, #860d07 0%, #b61209 100%);
  background: -o-linear-gradient(#b61209, #860d07);
}
.button.remove:hover,
a.button.remove:hover,
button.remove:hover,
input[type=submit].remove:hover {
  background: #9e1008;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #9e1008), color-stop(1, #ce150b));
  background: -ms-linear-gradient(bottom, #9e1008, #ce150b);
  background: -moz-linear-gradient(center bottom, #9e1008 0%, #ce150b 100%);
  background: -o-linear-gradient(#ce150b, #9e1008);
}
.button.remove:active,
a.button.remove:active,
button.remove:active,
input[type=submit].remove:active {
  background: #860d07;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #860d07), color-stop(1, #6d0b06));
  background: -ms-linear-gradient(bottom, #860d07, #6d0b06);
  background: -moz-linear-gradient(center bottom, #860d07 0%, #6d0b06 100%);
  background: -o-linear-gradient(#6d0b06, #860d07);
}
.wrapper {
  width: 100%;
  margin: 0 auto;
  height: 100%;
}


/*
 *
 Structure
 */

.loading-map,
.displaying-map {
  -webkit-transition: background-color 1s linear;
  -khtml-transition: background-color 1s linear;
  -moz-transition: background-color 1s linear;
  -o-transition: background-color 1s linear;
  transition: background-color 1s linear;
  background-color: #efefef;
}
.loading-map > * {
  -webkit-transition: opacity 1s linear;
  -khtml-transition: opacity 1s linear;
  -moz-transition: opacity 1s linear;
  -o-transition: opacity 1s linear;
  transition: opacity 1s linear;
  opacity: 0;
}
.displaying-map > * {
  opacity: 1;
}

/*#masthead*/

.loading-map #masthead {
  -webkit-transform: translate(0px, -100px);
  -khtml-transform: translate(0px, -100px);
  -moz-transform: translate(0px, -100px);
  -o-transform: translate(0px, -100px);
  transform: translate(0px, -100px);
}
.displaying-map #masthead {
  -webkit-transition: all 0.8s ease-in-out;
  -khtml-transition: all 0.8s ease-in-out;
  -moz-transition: all 0.8s ease-in-out;
  -o-transition: all 0.8s ease-in-out;
  transition: all 0.8s ease-in-out;
  -webkit-transform: translate(0, 0);
  -khtml-transform: translate(0, 0);
  -moz-transform: translate(0, 0);
  -o-transform: translate(0, 0);
  transform: translate(0, 0);
}
#masthead {
  padding: 0;
  position: relative!important;
  height: 100px!important;
  background: #fff !important;
  z-index: 998;
  -webkit-transition: all 0.2s ease-in-out;
  -khtml-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  transition: all 0.2s ease-in-out;
}
#masthead .site-meta {
  margin: 15px 0;
  -webkit-transition: margin 0.2s ease-in-out;
  -khtml-transition: margin 0.2s ease-in-out;
  -moz-transition: margin 0.2s ease-in-out;
  -o-transition: margin 0.2s ease-in-out;
  transition: margin 0.2s ease-in-out;
}
#masthead .site-meta h1 {
  margin: 0 auto;
  height: 100%;
  padding: 0;
}
#masthead .site-meta h1 {
  -webkit-transition: all 0.2s ease-in-out;
  -khtml-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  transition: all 0.2s ease-in-out;
  line-height: 0px;
  text-indent: -9999px;
  width: 100%;
  height: 50px;
  display: block;
  text-align: center;
}
#masthead .logo img {
  display: block;
  max-height: 72px;
  -webkit-transition: all 0.2s ease-in-out;
  -khtml-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  transition: all 0.2s ease-in-out;
}
#masthead .logo {
  float: left;
  margin: 14px;
}

#masthead .main-menu {
  float: right;
}
#masthead .main-menu #langnav {
  float: right;
  margin: 0 auto;
}
#masthead .main-menu #langnav ul {
  list-style: none;
  float: right;
  margin: 1em;
  width: 2em;
}
#masthead .main-menu #langnav ul li {
  margin: 0px;
}
#masthead .main-menu #langnav ul li a {
  font-size: 11px;
  display: block;
  text-transform: uppercase;
  background-color: rgb(103,52,8);
  padding: 0px 5px;
  margin-bottom: 2px;
  height: 18px;
  text-decoration: none;
  color: #fff;
}
#masthead .main-menu #langnav ul li a:hover,
#masthead .main-menu #langnav ul li a.active  {
  background-color: #FFF;
  color: rgb(103,52,8);
}
#masthead .city-selector {
  margin: 40px 0 0 20px;
  position: relative;
  -webkit-transition: margin 0.2s ease-in-out;
  -khtml-transition: margin 0.2s ease-in-out;
  -moz-transition: margin 0.2s ease-in-out;
  -o-transition: margin 0.2s ease-in-out;
  transition: margin 0.2s ease-in-out;
}
#masthead .city-selector .city-title {
  margin: 0;
  padding: 12px 15px 12px 10px;
  display: block;
  text-transform: uppercase;
  color: #f9f9f9;
  font-weight: 800;
  text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.5);
  cursor: default;
  font-size: 12px;
  line-height: 12px;
  z-index: 2;
  position: relative;
}
#masthead .city-selector .lsf {
  color: #555;
  font-size: 10px;
  line-height: 10px;
  margin-right: 8px;
  text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.3);
}
#masthead .city-selector .city-list {
  display: none;
  background: #f0f0f0;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #dddddd), color-stop(1, #f0f0f0));
  background: -ms-linear-gradient(bottom, #dddddd, #f0f0f0);
  background: -moz-linear-gradient(center bottom, #dddddd 0%, #f0f0f0 100%);
  background: -o-linear-gradient(#f0f0f0, #dddddd);
  -webkit-box-shadow: 0 0 15px rgba(0, 0, 0, 0.6);
  -khtml-box-shadow: 0 0 15px rgba(0, 0, 0, 0.6);
  -moz-box-shadow: 0 0 15px rgba(0, 0, 0, 0.6);
  -o-box-shadow: 0 0 15px rgba(0, 0, 0, 0.6);
  box-shadow: 0 0 15px rgba(0, 0, 0, 0.6);
  position: relative;
  z-index: 1;
  padding: 0;
  max-height: 204px;
  overflow-x: hidden;
  overflow-y: auto;
}
#masthead .city-selector .city-list::-webkit-scrollbar {
  width: 5px;
  background: #f0f0f0;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #dddddd), color-stop(1, #f0f0f0));
  background: -ms-linear-gradient(bottom, #dddddd, #f0f0f0);
  background: -moz-linear-gradient(center bottom, #dddddd 0%, #f0f0f0 100%);
  background: -o-linear-gradient(#f0f0f0, #dddddd);
}
#masthead .city-selector .city-list::-webkit-scrollbar-thumb {
  background: #333;
}
#masthead .city-selector .city-list li {
  margin: 0;
  padding: 0;
  border-bottom: 1px solid #ccc;
}
#masthead .city-selector .city-list li a {
  padding: 15px 10px;
  display: block;
  font-weight: 800;
  text-transform: uppercase;
  font-size: 10px;
  line-height: 10px;
  color: #555;
  text-decoration: none;
}
#masthead .city-selector .city-list li a:hover {
  background: #333333;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #222222), color-stop(1, #333333));
  background: -ms-linear-gradient(bottom, #222222, #333333);
  background: -moz-linear-gradient(center bottom, #222222 0%, #333333 100%);
  background: -o-linear-gradient(#333333, #222222);
  color: #f0f0f0;
}
#masthead .city-selector .city-list li:last-child {
  border-bottom: 0;
}
#masthead .city-selector .city-list li.tip {
  padding: 0 10px 10px;
  color: #999;
  font-style: italic;
  font-size: 11px;
}
#masthead .city-selector:hover .city-title {
  color: #333;
  background: #f0f0f0;
  background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #f0f0f0), color-stop(1, #cccccc));
  background: -ms-linear-gradient(bottom, #f0f0f0, #cccccc);
  background: -moz-linear-gradient(center bottom, #f0f0f0 0%, #cccccc 100%);
  background: -o-linear-gradient(#cccccc, #f0f0f0);
  text-shadow: 0 0 0;
  border-color: #f0f0f0;
}
#masthead .city-selector:hover .lsf {
  color: #333;
  text-shadow: 0 0 0;
}
#masthead .city-selector:hover .city-list {
  display: block;
}
#masthead #masthead-nav {
