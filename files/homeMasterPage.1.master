    <script>
        $(document).ready(function () {
            $("#owl-demo").owlCarousel({
                autoPlay: 3000, //Set AutoPlay to 3 seconds 
                items: 4,
                itemsDesktop: [640, 5],
                itemsDesktopSmall: [480, 2],
                navigation: true

            });
        });
    </script>
    <script src="js/jquery-scrolltofixed-min.js" type="text/javascript"></script>
    <script>
        
        function keyPress(val) {
            
            $('#hdnSearch').val($('#txtSearch').val());
        }
        $(document).ready(function () {

            // Dock the header to the top of the window when scrolled past the banner. This is the default behaviour.

            $('.header-two').scrollToFixed();
            // previous summary up the page.

            var summaries = $('.summary');
            summaries.each(function (i) {
                var summary = $(summaries[i]);
                var next = summaries[i + 1];

                summary.scrollToFixed({
                    marginTop: $('.header-two').outerHeight(true) + 10,
                    zIndex: 999
                });
            });
        });
        $(window).load(function () {
            $('.rating').rating();
            var productId = '<%=hdnlastProductId.Value%>';
            var status = '<%=hdnStatusToCheck.Value %>';
           
            if (productId != "" && status == 'True') {
                
                $('#myModal').modal('show');
                $('#userControlProduct').val(productId);
                $('#headingProduct').text('Product Review For:' + productId.split('~')[2])
            }          

        })
    </script>
