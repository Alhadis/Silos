.SH SYNOPSIS
.B #include <stringprep.h>
.sp
.BI "uint32_t * stringprep_ucs4_nfkc_normalize(uint32_t * " str ", ssize_t " len ");"
.SH ARGUMENTS
.IP "uint32_t * str" 12
