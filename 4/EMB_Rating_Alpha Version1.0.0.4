swagger: '2.0'
info:
  version: "1.0"
  title: Solartis Rate API for Embroker
  description: To get rate for Management Liability from Solartis API
  
host: embroker.alpha.solartis.net
basePath: /KnowledgeEngineV6_2/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Get Rates for the given input
      operationId: getRates
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName  
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string 
          default: "Rating"
        - name: EventVersion
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "1.0.0.4"
        - name: Policy
          in: body
          description: Policy content
          required: true
          schema:
            $ref: '#/definitions/Policy_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Policy_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
            
definitions:
  Policy_Request:
    type: object
    required:
      - ServiceRequestDetail
      - Policy
    properties:
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "36"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          UserName:
            type: string
            default: "EmbrokerAgent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          LanguageCode:
            type: string
            default: "en"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      Policy:
        type: object
        required:
          - ProductNumber
          - ProductVerNumber
          - EventName
          - EventVersion
          - SubmissionNumber
          - EffectiveDate
          - ExpirationDate
        
        properties:
          ProductNumber:
            title: "Product Number"
            type: string
            default: "EMBRKR"
          ProductVerNumber:
            title: "Product Ver Number"
            type: string
            default: "EMBRKR_2018_V1"
          EventName:
            title: "Event Name"
            type: string
            default: "Rating"
          EventVersion:
            title: "Event Version"
            type: string
            default: "1.0.0.4"
          SubmissionNumber:
            title: "SubmissionNumber"
            type: string
          EffectiveDate:
            title: "Effective Date"
            type: string
          ExpirationDate:
            title: "Expiration Date"
            type: string
          ManagementExperienceDiscountFactor:
            title: "Management Experience Discount Factor"
            type: string
          FinancialAnalysisDiscountFactor:
            title: "Financial Analysis Discount Factor"
            type: string
          PartnerCode:
            title: "Partner Code"
            type: string
          Coverage:
            type: array
            items:
              properties:
                CoverageDetail:
                  type: object
                  required:
                    - CoverageName
                    - LimitOfLiability
                    - Retention
                  properties:
                    IncludeInTotal:
                      title: "Include In Total"
                      type: string
                      enum: ["Yes", "No"]
                    CoverageName:
                      title: "Coverage Name"
                      type: string
                      enum: ["Directors and Officers","EPLI","Fiduciary"]
                    LimitOfLiability:
                      title: "Limit Of Liability"
                      type: string
                      enum: ["1000000","2000000","3000000","4000000","5000000"]
                    Retention:
                      title: "Retention"
                      type: string
                    
      OwnerId:
        title: "Owner ID"
        type: string
        default: "36"
        
